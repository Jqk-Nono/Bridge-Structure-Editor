<Window x:Class="新建桥类.选择导入或新建"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="选择导入或新建" SizeToContent="WidthAndHeight" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <ResourceDictionary Source="style.xaml" />
    </Window.Resources>

    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="新建" Click="新建Button_CLick"/>
            <Button Content="导入" Click="导入Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
